{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.1",
    "copy-webpack-plugin": "^10.2.4",
    "html-webpack-plugin": "^5.5.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "redux-persist": "^6.0.0",
    "webpack": "^5.68.0",
    "webpack-merge": "^5.8.0"
  },
  "scripts": {
    "start": "concurrently --kill-others \"npm run server\" \"webpack serve -c config/webpack/dev.js\"",
    "watch": "webpack --watch",
    "build": "webpack -c config/webpack/prod.js",
    "build-dev": "webpack",
    "clean": "rmdir /s /q dist",
    "analyze": "webpack --analyze -c config/webpack/analyze.js",
    "server": "json-server -w ../server/db.json -p 3001",
    "_lint": "eslint --config ./.eslintrc.json --ext js --ext jsx ",
    "lint": "npm run _lint -- ./src/",
    "lint:fix": "npm run _lint -- --fix ./src/",
    "format": "prettier --write \"**/*{js,jsx,css,scss,md,json}\" --config ./.prettierrc",
    "lint:css": "stylelint \"**/*{css,scss}\" --fix --ignore-path .gitignore "
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "autoprefixer": "^10.4.2",
    "babel-eslint": "^10.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "imagemin-webpack-plugin": "^2.1.5",
    "json-server": "^0.17.0",
    "markdown-loader": "^8.0.0",
    "mini-css-extract-plugin": "^2.4.5",
